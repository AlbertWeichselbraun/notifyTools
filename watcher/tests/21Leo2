<script>
$(document).ready(function() {

    var rangeSizeSlider = $( "#slider-range-size" );
    var rangeSizeRooms  = $( "#slider-range-rooms" );
    var rangeSizeStorey = $( "#slider-range-storey" );
    var rangeSizePrice  = $( "#slider-range-price" );
    var minRooms        = $('input[name="data[Slider][minRooms]"]').val();
    var maxRooms        = $('input[name="data[Slider][maxRooms]"]').val();
    var minSize         = $('input[name="data[Slider][minSize]"]').val();
    var maxSize         = $('input[name="data[Slider][maxSize]"]').val();
    var minPrice        = $('input[name="data[Slider][minPrice]"]').val();
    var maxPrice        = $('input[name="data[Slider][maxPrice]"]').val();
    var minStoreyOld    = $('input[name="data[Slider][minStoreyOld]"]').val();
    var maxStoreyOld    = $('input[name="data[Slider][maxStoreyOld]"]').val();

    /*
    console.log('maxRooms ' + maxRooms);
    console.log('minRooms ' + minRooms);
    console.log('minSize ' + minSize);
    console.log('maxSize ' + maxSize);
    console.log('minPrice ' + minPrice);
    console.log('maxPrice ' + maxPrice);
    console.log('minStoreyOld ' + minStoreyOld);
    console.log('maxStoreyOld ' + maxStoreyOld);
    */

    rangeSizeRooms.slider({
      range: "max",
      range: true,
      min: 2,           // min value
      max: 5,         // max value
      values: [minRooms, maxRooms], // default values
      step : 1,
      slide: function( event, ui ) {
        // set new values on slide
        $('.slider-range-min-value[data-slider="slider-range-rooms"]').text(ui.values[0]);
        $('.slider-range-max-value[data-slider="slider-range-rooms"]').text(ui.values[1]);
        $('input[name="data[Slider][minRooms]"]').val(ui.values[0]);
        $('input[name="data[Slider][maxRooms]"]').val(ui.values[1]);
      }
    });

    // set default values on init
    $('.slider-range-min-value[data-slider="slider-range-rooms"]').text(rangeSizeRooms.slider('values', 0 ));
    $('.slider-range-max-value[data-slider="slider-range-rooms"]').text(rangeSizeRooms.slider('values', 1 ));

    rangeSizeSlider.slider({
      range: "max",
      range: true,
      min: 55.00,           // min value
      max: 122.00,         // max value
      values: [minSize, maxSize], // default values
      step : 1,
      slide: function( event, ui ) {
        // set new values on slide
        $('.slider-range-min-value[data-slider="slider-range-size"]').text(ui.values[0] + ' m²');
        $('.slider-range-max-value[data-slider="slider-range-size"]').text(ui.values[1] + ' m²');
        $('input[name="data[Slider][minSize]"]').val(ui.values[0]);
        $('input[name="data[Slider][maxSize]"]').val(ui.values[1]);
      }
    });

    // set default values on init
    $('.slider-range-min-value[data-slider="slider-range-size"]').text(rangeSizeSlider.slider('values', 0 ) + ' m²');
    $('.slider-range-max-value[data-slider="slider-range-size"]').text(rangeSizeSlider.slider('values', 1 ) + ' m²');

    // generate names for the values.. slider only accepts/slides numbers but we want strings to be displayed.
    // so, we "simply" build a translation table then.
    maxOG              = 4;
    maxDG              = 2;
    projectStoreys     = {};
    projectStoreys[-1] = "KG";
    projectStoreys[0]  = "EG";
    for ($i = 1; $i <= maxOG+maxDG; $i++) {
      if ($i <= maxOG)
        projectStoreys[$i] = $i+'.OG';
      else if ($i > maxOG)
        projectStoreys[$i] = ($i-maxOG) +'.DG';
    }
    // handle empty maxStoreyOld (on init)
    if (!maxStoreyOld)
      maxStoreyOld = maxOG+maxDG;

    rangeSizeStorey.slider({
      range: "max",
      range: true,
      min: -1,
      max: maxOG + maxDG,         // max value
      values: [minStoreyOld, maxStoreyOld], // position of slider at init, or "default values", in our case, index of projectStoreys object.
      step : 1,
      slide: function( event, ui ) {
        // set new values on slide
        $('.slider-range-min-value[data-slider="slider-range-storey"]').text( projectStoreys[ui.values[0]] );
        $('.slider-range-max-value[data-slider="slider-range-storey"]').text( projectStoreys[ui.values[1]] );
        $('input[name="data[Slider][minStoreyOld]"]').val(ui.values[0]);
        $('input[name="data[Slider][maxStoreyOld]"]').val(ui.values[1]);
        if (ui.values[0] <= maxOG) {
          $('input[name="data[Slider][minStorey]"]').val(ui.values[0]);
          $('input[name="data[Slider][minStoreyType]"]').val("storey");
        }
        else {
          $('input[name="data[Slider][minStorey]"]').val(ui.values[0] - maxOG);
          $('input[name="data[Slider][minStoreyType]"]').val("attic"); 
        }
        if (ui.values[1] <= maxOG) {
          $('input[name="data[Slider][maxStorey]"]').val(ui.values[1]);
          $('input[name="data[Slider][maxStoreyType]"]').val("storey");
        }
        else {
          $('input[name="data[Slider][maxStorey]"]').val(ui.values[1] - maxOG);
          $('input[name="data[Slider][maxStoreyType]"]').val("attic"); 
        }
      }
    });
    // set default values on init
    $('.slider-range-min-value[data-slider="slider-range-storey"]').text( projectStoreys[$("#slider-range-storey").slider('values', 0)]);
    $('.slider-range-max-value[data-slider="slider-range-storey"]').text( projectStoreys[$("#slider-range-storey").slider('values', 1)] );
    $('input[name="data[Slider][minStoreyType]"]').val('storey');
    $('input[name="data[Slider][maxStoreyType]"]').val('attic');
    $('input[name="data[Slider][maxStorey]"]').val($("#slider-range-storey").slider('values', 1));
    $('input[name="data[Slider][minStorey]"]').val($("#slider-range-storey").slider('values', 0));
     

    rangeSizePrice.slider({
      range: "max",
      range: true,
      min: 131900.00,           // min value
      max: 349900.00,         // max value
      values: [minPrice, maxPrice], // default values
      step : 1,
      slide: function( event, ui ) {
        // set new values on slide
        $('.slider-range-min-value[data-slider="slider-range-price"]').text( '€ ' + format("#.###,00", ui.values[0]) );
        $('.slider-range-max-value[data-slider="slider-range-price"]').text( '€ ' + format("#.###,00", ui.values[1]) );
        $('input[name="data[Slider][minPrice]"]').val(ui.values[0]);
        $('input[name="data[Slider][maxPrice]"]').val(ui.values[1]);
      }
    });


    // set default values on init
    $('.slider-range-min-value[data-slider="slider-range-price"]').text( '€ ' + format( "#.###,00", rangeSizePrice.slider('values', 0)) );
    $('.slider-range-max-value[data-slider="slider-range-price"]').text( '€ ' + format( "#.###,00", rangeSizePrice.slider('values', 1)) );



    // handle call
    rangeSizeSlider.on( "slidestop", function( event, ui ) { do_filter_tops() } );
    rangeSizeRooms.on( "slidestop", function( event, ui ) { do_filter_tops() } );
    rangeSizeStorey.on("slidestop", function( event, ui ) { do_filter_tops() } );
    rangeSizePrice.on( "slidestop", function( event, ui ) { do_filter_tops() } );

    // handle tops filter
    function do_filter_tops() {
        // disable sliders
        rangeSizeSlider.slider('disable');
        rangeSizeRooms.slider('disable');
        rangeSizeStorey.slider('disable');
        rangeSizePrice.slider('disable');

        // post data
        $.post($('#filterTopsForm').prop('action'), $('#filterTopsForm').serialize(), function(response) {
            // reanable sliders
            rangeSizeSlider.slider('enable');
            rangeSizeRooms.slider('enable');
            rangeSizeStorey.slider('enable');
            rangeSizePrice.slider('enable');

            // update list only if there are tops
            // if (response != 69)
            $('.project-tops').html(response);


                $('.object-grid-tops li:nth-child(3n)').addClass('last');


        });
    }

    // get size of an object
    Object.size = function () {
      var len = this.length ? --this.length : -1;
        for (var k in this)
          len++;
      return len;
    }



    // set default value
