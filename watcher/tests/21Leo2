<script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
<script type="text/javascript">
var map;
function initialize() {
    geocoder = new google.maps.Geocoder();
    var myOptions = {
        zoom: 16,
        center: new google.maps.LatLng(-34.397, 150.644),
        mapTypeId: google.maps.MapTypeId.ROADMAP
    };
    map = new google.maps.Map(document.getElementById('map_canvas'), myOptions);



}

google.maps.event.addDomListener(window, 'load', initialize);


function codeAddress() {
				var address = 'Sebastian-Kohl-Gasse 14, Wien';
		var tooltip = 'Sebastian-Kohl-Gasse 6-12';
		
    geocoder.geocode( { 'address': address}, function(results, status) {
        if (status == google.maps.GeocoderStatus.OK) {
            map.setCenter(results[0].geometry.location);

            var image = new google.maps.MarkerImage('http://www.wvg.at/frontend/img/map_icon.png',
                  // This marker is 20 pixels wide by 32 pixels tall.
                  new google.maps.Size(124, 124),
                  // The origin for this image is 0,0.
                  new google.maps.Point(0,0),
                  // The anchor for this image is the base of the flagpole at 0,32.
                  new google.maps.Point(62, 124));


            var marker = new google.maps.Marker({
                    map: map,
                    position: results[0].geometry.location,
                    title: tooltip,
                    icon: image
            });

            var infowindow = new google.maps.InfoWindow({
                content: tooltip
            });
            //google.maps.event.addListener(marker, 'click', function() { infowindow.open(map,marker); });
             map.setCenter(results[0].geometry.location);
        } else {
            /*alert("Geocode war nicht erfolgreich! Grund: " + status);*/
             alert('Keine Adresse oder Top angegeben. Bitte überprüfen Sie ihre eingaben.');
        }
    });
}
</script>
<script>
$(document).ready(function() {

    // set last item
    $('.object-grid li:nth-child(3n)').addClass('last');

    // load quickrequests form
    $('#project-quickrequest').load('/quickrequests/add/39', function() {
        //laoded
    });

    // load tellafriend form
    $('#project-tellafriend').load('/tellafriends/send', function() {
        //laoded
    })

    // init fancybox
    $('.fancybox').fancybox();

    // use hash in url
     var gotoHashTab = function (customHash) {
            var hash = customHash || location.hash;
            var hashPieces = hash.split('?'),
                activeTab = $('[href=' + hashPieces[0] + ']');
            activeTab && activeTab.tab('show');
        }

    // onready go to the tab requested in the page hash
    gotoHashTab();

    // show tell a friend form
    $('.show-tellafriend').on('click', function() {
        $('#tellafriendForm').show();
        return false;
    })

    // when the nav item is selected update the page hash
    $('.nav a').on('shown', function (e) {
        window.location.hash = e.target.hash;
    })

    // when a link within a tab is clicked, go to the tab requested
    $('.tab-pane a').click(function (event) {
        if (event.target.hash) {
            gotoHashTab(event.target.hash);
        }
    });

    // toggle icons in accordion
    $('.accordion').on('shown', function(e) {

        $(e.target).parent().parent().find('i').toggleClass('icon-chevron-up').toggleClass('icon-chevron-down');
    });

    $('.accordion').on('hidden', function(e) {
        $(e.target).parent().parent().find('i').toggleClass('icon-chevron-up').toggleClass('icon-chevron-down');
    });


    // switch tabs
    $('a[data-toggle="tab"]').on('shown', function (e) {
        // update gogole maps
        if ($(e.target).attr('href') == '#project-location') {
            setTimeout(function() {
                codeAddress();
                google.maps.event.trigger(map, 'resize');
            }, 400)
        }

         if ($(e.target).attr('href') == '#project-quickrequest') {
                $('.aside-menu li.current, .aside-menu li.active').removeClass('current active');
            }
    });

    // download pdf
    $('#project-plans ul li a').attr('target', '_blank');

        setTimeout(function() {
        $('.spotlight').backstretch('http://www.wvg.at/media/transfer/img/aussenansicht_21leo26_ohne_text_3.jpg');
    }, 100)
    });
</script>

